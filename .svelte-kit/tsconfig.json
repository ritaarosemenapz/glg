{
	"compilerOptions": {
		"baseUrl": "..",
		"paths": {
			"src": [
				"src"
			],
			"src/*": [
				"src/*"
			],
			"img": [
				"src/routes/images"
			],
			"img/*": [
				"src/routes/images/*"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"importsNotUsedAsValues": "error",
		"isolatedModules": true,
		"preserveValueImports": true,
		"lib": [
			"esnext",
			"DOM"
		],
		"moduleResolution": "node",
		"module": "esnext",
		"target": "esnext"
	},
	"include": [
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"./**"
	]
}